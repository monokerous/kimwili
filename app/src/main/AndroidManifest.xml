<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeannebertoux.kimwili">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controller.Accueil">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.ActiviteActivity" />
        <activity android:name=".Controller.AjouterActiviteActivity" />
        <activity android:name=".Controller.NiveauDouleurActivity" />
        <activity android:name=".Controller.AjouterNiveauDouleur" />
        <activity android:name=".Controller.StatistiqueActivity" />
        <activity android:name=".Controller.SessionActivity" />
        <activity android:name=".Controller.SessionStart"></activity>
        <activity android:name=".Controller.SelectSession" />
        <activity android:name=".Controller.MesDonneesActivity" />
        <activity android:name=".Controller.SessionSelectedActivity"></activity>
    </application>

</manifest>